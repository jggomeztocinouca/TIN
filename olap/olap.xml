<Schema name="EducacionEspaña">
    <!-- Dimensiones -->
    <Dimension name="CCAA">
        <Hierarchy hasAll="true" allMemberName="Todas las Comunidades">
            <Table name="D_CCAA"/>
            <Level name="Comunidad Autonoma" column="Comunidad_Autonoma" uniqueMembers="true"/>
        </Hierarchy>
    </Dimension>

    <Dimension name="Year">
        <Hierarchy hasAll="true" allMemberName="Todos los Años">
            <Table name="D_YEAR"/>
            <Level name="Año" column="ID_year" type="Numeric" uniqueMembers="true"/>
        </Hierarchy>
    </Dimension>

    <Dimension name="Sexo">
        <Hierarchy hasAll="true" allMemberName="Todos los Sexos">
            <Table name="D_SEXO"/>
            <Level name="Sexo" column="SEXO" uniqueMembers="true"/>
        </Hierarchy>
    </Dimension>

    <Dimension name="Edad">
        <Hierarchy hasAll="true" allMemberName="Todas las Edades">
            <Table name="D_EDAD"/>
            <Level name="Rango Edad" column="RANGO" uniqueMembers="true"/>
        </Hierarchy>
    </Dimension>

    <Dimension name="Centros">
        <Hierarchy hasAll="true" allMemberName="Todos los Centros">
            <Table name="D_CENTROS"/>
            <Level name="Tipo Centro" column="TIPO" uniqueMembers="true"/>
        </Hierarchy>
    </Dimension>

    <Dimension name="Enseñanza">
        <Hierarchy hasAll="true" allMemberName="Todos los Tipos">
            <Table name="D_ENSENANZA"/>
            <Level name="Tipo Enseñanza" column="TIPO" uniqueMembers="true"/>
        </Hierarchy>
    </Dimension>

    <!-- Cubos -->
    <Cube name="AbandonoEscolar">
        <Table name="H_ABANDONO"/>
        <DimensionUsage name="CCAA" source="CCAA" foreignKey="ID_CCAA"/>
        <DimensionUsage name="Year" source="Year" foreignKey="ID_year"/>
        <DimensionUsage name="Sexo" source="Sexo" foreignKey="ID_SEXO"/>
        <Measure name="Tasa de Abandono" column="Tasa_Abandono" aggregator="avg"/>
    </Cube>

    <Cube name="NumeroAlumnos">
        <Table name="H_NUMALUMNOS"/>
        <DimensionUsage name="Year" source="Year" foreignKey="YEAR_ID"/>
        <DimensionUsage name="Sexo" source="Sexo" foreignKey="SEXO_ID"/>
        <DimensionUsage name="CCAA" source="CCAA" foreignKey="CCAA_ID"/>
        <DimensionUsage name="Centros" source="Centros" foreignKey="CENTRO_ID"/>
        <DimensionUsage name="Enseñanza" source="Enseñanza" foreignKey="ENSENANZA_ID"/>
        <Measure name="Número de Alumnos" column="nALUMNOS" aggregator="sum"/>
    </Cube>

    <Cube name="NumeroProfesores">
        <Table name="H_NUMPROFESORES"/>
        <DimensionUsage name="Year" source="Year" foreignKey="YEAR_ID"/>
        <DimensionUsage name="Sexo" source="Sexo" foreignKey="SEXO_ID"/>
        <DimensionUsage name="CCAA" source="CCAA" foreignKey="CCAA_ID"/>
        <DimensionUsage name="Centros" source="Centros" foreignKey="CENTRO_ID"/>
        <DimensionUsage name="Edad" source="Edad" foreignKey="EDAD_ID"/>
        <Measure name="Número de Profesores" column="nPROFESORES" aggregator="sum"/>
    </Cube>
</Schema>